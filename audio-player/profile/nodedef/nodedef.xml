<nodeDefs>

<!-- The node server control Node-->
  <nodeDef id="mpgPlayer" nls="mpgPlayer">
    <sts>
      <st id="ST" editor="playStatus" />
      <st id="GV0" editor="mpgNames" />
      <st id="GV1" editor="btStatus" />
      <st id="GV2" editor="output" />
	  </sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="PLAY">
          <p id="PLAYLIST" editor="mpgNames" />
        </cmd>
        <cmd id="STOP">
        </cmd>
        <cmd id="BT"> 
          <p id="BTSTATUS" editor="btStatus" />
        </cmd>
        <cmd id="OUTPUT">
          <p id="OUTPUT" editor="output" />
        </cmd>
      </accepts>
    </cmds>
  </nodeDef>

</nodeDefs>