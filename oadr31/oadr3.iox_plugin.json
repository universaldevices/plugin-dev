{
    "plugin": {
          "name":"oadr3",
          "description":"OpenADR 3.1 VEN Plugin for IoX",
          "publisher":"UD",
          "language": "python3",
          "executableName":"oadr3.py",
          "installScript":"install.sh",
          "documentationLink":"docs",
          "licenseLink":"lic",
          "shortPoll":5,
          "longPoll":30,
          "profileVersion":"3.0.0",
          "initialLogLevel":"DEBUG",
          "enableOAUTH2":false,
          "worksOnPolisy":true,
          "worksOnEisy":true,
          "requiresIoXAccess": true,
          "enableFileUpload": false,
          "nodesAreStatic": true,
          "requirements": ["oadr30_ven_ud","tzlocal", "isodate", "pytz", "nucore-ai"],
          "version": "1.0.5"
    },
    "editors": [
        {
            "id": "ghg",
            "min": 0,
            "max": 1,
            "uom": "Raw 2-Byte unsigned value | 108",
            "precision": 4,
            "step": 0.01 
        },
        {
            "id": "price",
            "min": -10,
            "max": 10,
            "uom": "Dollar | 103",
            "precision": 4,
            "step": 0.001
        },
        {
            "id": "comfort_level",
            "min": 0,
            "max": 1,
            "uom": "Index | 25",
            "index_names": [
                "Max Comfort (Least Savings) | 0",
                "Max Savings (Least Comfort) | 1"
            ]
        },
        {
            "id": "current_grid_status",
            "min": 0,
            "max": 3,
            "uom": "Index | 25",
            "index_names": [
                "Normal | 0",
                "Moderate | 1",
                "High | 2",
                "DR | 3"
            ]
        }
    ],
    "nodedefs": [
        {
            "id":"oadr3ven",
            "name":"Oadr3 Energy Optimizer",
            "icon":"EnergyMonitor",
            "properties": [
                {
                    "id":"ST",
                    "name": "Price",
                    "editor": {
                        "idref": "price"
                    },
                    "is_settable": false
                },
                {
                    "id": "GHG",
                    "name": "GHG",
                    "editor": {
                        "idref": "ghg"
                    } ,
                    "is_settable": false
                },
                {
                   "id": "CL",
                   "name": "Comfort Level",
                   "editor": {
                       "idref": "comfort_level"
                   },
                   "is_settable": true 
                },
                {
                   "id": "CGS",
                   "name": "Current Grid Status",
                   "editor": {
                       "idref": "current_grid_status"
                   },
                   "is_settable": false 
                }
            ],
            "commands": {
                "accepts": [],
                "sends": []
            } 
        }
    ]
}
